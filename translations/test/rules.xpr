@name REGISTER
@includes {
  /home/register
}

% アカウント登録画面
FORM //form[1]/div[1] {
  % タイトル
  TITLE /h4[1]
  % 入力欄
  REGISTER_INPUT /div[2:3]/input.e1buxcrw1[1] * [placeholder]
  % 誕生月
  AGE_MONTH //select#age_month/option *
  % 利用規約
  CONSENT /div[5]/label.eazy3iu0[1] * {
    /p[1]/text()[1,4]
    /p[1]/a[1:3]
  }
}